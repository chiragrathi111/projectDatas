.ware-house-data-widget {
	display: inline-flex;
	padding: 8px 24px 8px;
	align-items: stretch;
	gap: 24px;
	width: 100%;
    /*justify-content: space-between;*/
}

.ware-house-select-widget {
	margin-left: 24px;
}

.ware-house-select-widget-cont {
	width: 95%;
	display: flex;
	gap : 32px;
	align-items: center;
	margin: 16px 0px;
}

.ware-house-data-widget td {
	padding: 4px 4px 4px 4px;
}

.dashboard-widget-labels {
	border-radius: 4px;
	padding:8px;
	padding-bottom: 0px;
	color: #081735 !important;
	text-align: center;
	font-size: 14px;
	font-style: normal;
	font-weight: 700;
	line-height: 28px; /* 140% */
	letter-spacing: 0.15px;
}

.dashboard-widget-labels-basic-stats {
	border-radius: 4px;
	padding:8px;
	color: #081735 !important;
	text-align: center;
	font-size: 14px;
	font-style: normal;
	font-weight: 700;
	line-height: 28px; /* 140% */
	letter-spacing: 0.15px;
}

.dashboard-widget-labels-border-purple {
    border: 5px solid #CABDFF;
    border-radius: 4px;
    background-color: #CABDF;
}

.dashboard-widget-labels-border-light-blue {
	border: 5px solid #B1E5FC;
    border-radius: 4px;
    background-color: #B1E5FC;
}

.dashboard-widget-labels-border-orange {
	border: 5px solid #FFBC99;
    border-radius: 4px;
    background-color: #FFBC99;
}

.dashboard-widget-count {
	color: #8F95B2 !important;
	text-align: center;
	font-size: 32px !important;
	font-style: normal;
	font-weight: 500;
	line-height: 40px; /* 116.667% */
	letter-spacing: 0.15px;
}

.dashboard-widget-currency-type {
	color: #8F95B2 !important;
	text-align: center;
	font-size: 24px !important;
	font-style: normal;
	font-weight: 500;
	line-height: 56px; /* 116.667% */
	letter-spacing: 0.15px;
}

.dashboard-widget-occupancy {
	font-size:48px !important;
	font-weight: 500;
	color:#8F95B2 !important;
	display: flex;
	padding: 16px 24px;
	flex-direction: column;
	align-items: center;
	gap: 32px;
	font-style: normal;
	line-height: 56px; /* 116.667% */
	letter-spacing: 0.15px;
}

.dashboard-widget-lables-count {
	font-size:48px !important;
	font-weight: 500;
	color:#8F95B2 !important;
	display: flex;
	flex-direction: column;
	align-items: center;
	gap: 32px;
	font-style: normal;
	line-height: 56px; /* 116.667% */
	letter-spacing: 0.15px;
}

.dashboard-widget-lables-count-cont {
	display:flex;
	padding: 8px 16px;
	align-items: baseline;
}

.dashboard-widget-labels-count-text {
	font-weight: 400;
	font-size: 14px !important;
	color:#8F95B2 !important;
	font-style: normal;
	line-height: 22px; /* 157.143% */
	letter-spacing: 0.15px;
}

.dashboard-widget-text-count-cont {
	display: flex;
	padding: 8px 16px;
	flex-direction: column;
	align-items: center;
	/*gap: 8px;*/
    border-radius: 8px;
	background: #F8FAFC;
}

.dashboard-widget-text-count-cont-basic-stats {
	display: flex;
	padding: 8px 16px 4px 16px;
	flex-direction: column;
	align-items: center;
	gap: 8px;
    border-radius: 8px;
	background: #F8FAFC;
}

.dashboard-widget-lables-stock-check-cont {
	display: flex;
	flex-direction: column;
	align-items: center;
	gap: 8px;
    border-radius: 8px;
	background: #F8FAFC;
}

